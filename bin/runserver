#!/usr/bin/env node
var node = process.argv[0] === 'node',
    settings = node ? process.argv[2] : process.argv[1],
    port = node ? process.argv[3] : process.argv[2],
    sys = require('sys');

var runserver = require('wilson').runserver;

require.paths.push(process.cwd());
sys.debug(require.paths);

runserver(settings, port);

